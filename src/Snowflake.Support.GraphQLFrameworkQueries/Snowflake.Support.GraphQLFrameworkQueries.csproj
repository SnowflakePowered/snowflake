<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <DocumentationFile>$(OutputPath)$(AssemblyName).xml</DocumentationFile>
    <_SnowflakeUseDevelopmentSDK>true</_SnowflakeUseDevelopmentSDK>
  </PropertyGroup>
  
  <ItemGroup>
    <PackageReference Include="Snowflake.Framework.Sdk" Version="1.0.0-alpha.latest"/>
    <AdditionalFiles Include="..\stylecop.json" />
  </ItemGroup>
  
<ItemGroup>
  <Folder Include="Types\Execution\" />
</ItemGroup>
  <PropertyGroup>
    <CodeAnalysisRuleSet>..\Snowflake.ruleset</CodeAnalysisRuleSet>
    <NoWarn>1701;1702;1591;</NoWarn>
  </PropertyGroup>
</Project>
