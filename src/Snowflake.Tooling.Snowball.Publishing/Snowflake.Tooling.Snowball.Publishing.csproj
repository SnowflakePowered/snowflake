      <Version>4.0.0-rc-2048</Version>
    </PackageReference>
    <PackageReference Include="NuGet.Repositories">
      <Version>4.0.0-rc-2048</Version>
    </PackageReference>
    <PackageReference Include="Octokit">
      <Version>0.23.0</Version>
